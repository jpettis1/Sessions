{
  "name": "sessions",
  "version": "1.0.0",
  "description": "Athletic training and consulting application, connecting coaches and athletes for better performance outcomes.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "NODE_ENV=production nodemon ./server/server.js",
    "dev": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --hot --open --progress --color\" \"nodemon ./server/server.js\"",
    "build": "webpack",
    "lint": "eslint .",
    "lint:fix": "eslint --fix './**/*.{js,jsx}'",
    "format": "prettier --write './**/*.{js,jsx,css,md,json}' --config ./.prettierrc"
  },
  "author": "Jeff Pettis",
  "license": "ISC",
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "./setupJest.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css)$": "./mocks/fileMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!)"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@devexpress/dx-react-chart": "^3.0.6",
